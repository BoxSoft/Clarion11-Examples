          LIST,AT(8,22,424,147),USE(?Browse:1),HVSCROLL,FORMAT('32L(2)|M~GUID~C(0)@s16@' & |
            '32L(2)|M~Cus. Guid~C(0)@s16@35R(2)|M~Invoice #~C(0)@n07@40R(2)|M~Date~C(0)' & |
            '@d10@32L(2)|M~Shipped~@s1@41R(2)|M~Total~C(0)@n-11.2@40L(2)|M~First Name~@' & |
            's100@40L(2)|M~Last Name~@s100@80L(2)|M~Street~@s255@80L(2)|M~City~@s100@21' & |
            'L(2)|M~State~@s100@'),FROM(Queue:Browse:1),IMM,MSG('Browsing the Invoice file'), |
            #FIELDS(Inv:GUID,Inv:CustomerGuid,Inv:InvoiceNumber,Inv:Date,Inv:OrderShipped, |
            Inv:Total,Inv:FirstName,Inv:LastName,Inv:Street,Inv:City,Inv:State),#ORIG(?List), |
            #SEQ(1),#ORDINAL(1)
          BUTTON('&View'),AT(117,180,49,14),USE(?View:2),LEFT,ICON('WAVIEW.ICO'),FLAT,MSG('View Record'), |
            TIP('View Record'),#ORIG(?View),#SEQ(2),#ORDINAL(2)
          BUTTON('&Insert'),AT(170,180,49,14),USE(?Insert:3),LEFT,ICON('WAINSERT.ICO'),FLAT, |
            MSG('Insert a Record'),TIP('Insert a Record'),#LINK(?Change:3),#ORIG(?Insert), |
            #SEQ(3),#ORDINAL(3)
          BUTTON('&Change'),AT(223,180,49,14),USE(?Change:3),LEFT,ICON('WACHANGE.ICO'),DEFAULT, |
            FLAT,MSG('Change the Record'),TIP('Change the Record'),#LINK(?Delete:3),#ORIG(?Change), |
            #SEQ(3),#ORDINAL(4)
          BUTTON('&Delete'),AT(276,180,49,14),USE(?Delete:3),LEFT,ICON('WADELETE.ICO'),FLAT, |
            MSG('Delete the Record'),TIP('Delete the Record'),#LINK(?Insert:3),#ORIG(?Delete), |
            #SEQ(3),#ORDINAL(5)
          SHEET,AT(4,4,436,172),USE(?CurrentTab),#ORIG(CurrentTab),#ORDINAL(6)
            TAB('&GuidKey'),USE(?Tab:2),#ORDINAL(7)
            END
            TAB('&One Customer'),USE(?Tab:3),#ORDINAL(8)
              BUTTON('Select Customer'),AT(8,180,,14),USE(?SelectCustomer),LEFT,ICON('WAPARENT.ICO'), |
