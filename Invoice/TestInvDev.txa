[END]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
    MaxRandom = SIZE(Latin) - 31
    RandPos=RANDOM(1,MaxRandom)

