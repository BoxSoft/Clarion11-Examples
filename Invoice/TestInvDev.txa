[WINDOW]
QuickWindow WINDOW('Form CustomerCompany'),AT(,,358,167),FONT('Segoe UI',9,COLOR:Black,FONT:regular, |
          CHARSET:DEFAULT),RESIZE,CENTER,GRAY,IMM,MDI,HLP('UpdateCustomerCompany'),SYSTEM
          SHEET,AT(4,4,350,137),USE(?CurrentTab),#ORIG(CurrentTab),#ORDINAL(1)
            TAB('&General'),USE(?Tab:1),#ORDINAL(2)
              PROMPT('Company Name:'),AT(8,34),USE(?CusCom:CompanyName:Prompt),TRN,#LINK(?CusCom:CompanyName), |
                #ORIG(?CusCom:CompanyName:Prompt),#ORDINAL(3)
              ENTRY(@s100),AT(64,34,286,10),USE(CusCom:CompanyName),#LINK(?CusCom:CompanyName:Prompt), |
                #ORIG(CusCom:CompanyName),#ORDINAL(4)
              PROMPT('&Street:'),AT(8,48),USE(?CusCom:Street:Prompt),TRN,#LINK(?CusCom:Street), |
                #ORIG(?CusCom:Street:Prompt),#ORDINAL(5)
              TEXT,AT(64,48,286,30),USE(CusCom:Street),MSG('Enter the first line addres' & |
                's of customer'),#LINK(?CusCom:Street:Prompt),#ORIG(CusCom:Street),#ORDINAL(6)
              PROMPT('&City:'),AT(8,82),USE(?CusCom:City:Prompt),TRN,#LINK(?CusCom:City),#ORIG(?CusCom:City:Prompt), |
                #ORDINAL(7)
              ENTRY(@s100),AT(64,82,286,10),USE(CusCom:City),MSG('Enter  city of customer'), |
                #LINK(?CusCom:City:Prompt),#ORIG(CusCom:City),#ORDINAL(8)
              PROMPT('&State:'),AT(8,96),USE(?CusCom:State:Prompt),TRN,#LINK(?CusCom:State), |
                #ORIG(?CusCom:State:Prompt),#ORDINAL(9)
              ENTRY(@s100),AT(64,96,286,10),USE(CusCom:State),MSG('Enter state of customer'), |
                #LINK(?CusCom:State:Prompt),#ORIG(CusCom:State),#ORDINAL(10)
              PROMPT('&Zip Code:'),AT(8,110),USE(?CusCom:PostalCode:Prompt),TRN,#LINK(?CusCom:PostalCode), |
                #ORIG(?CusCom:PostalCode:Prompt),#ORDINAL(11)
              ENTRY(@s100),AT(64,110,286,10),USE(CusCom:PostalCode),MSG('Enter zipcode ' & |
                'of customer'),TIP('Enter zipcode of customer'),#LINK(?CusCom:PostalCode:Prompt), |
                #ORIG(CusCom:PostalCode),#ORDINAL(12)
              PROMPT('Mobile Phone:'),AT(8,124),USE(?CusCom:Phone:Prompt),TRN,#LINK(?CusCom:Phone), |
                #ORIG(?CusCom:Phone:Prompt),#ORDINAL(13)
              ENTRY(@s100),AT(64,124,286,10),USE(CusCom:Phone),#LINK(?CusCom:Phone:Prompt), |
                #ORIG(CusCom:Phone),#ORDINAL(14)
              PROMPT('GUID:'),AT(9,22),USE(?CusCom:GUID:Prompt),TRN,#ORDINAL(15)
              ENTRY(@s16),AT(64,21,78,10),USE(CusCom:GUID),SKIP,#ORDINAL(16)
            END
          END
          BUTTON('&OK'),AT(247,147,49,14),USE(?OK),LEFT,ICON('WAOK.ICO'),DEFAULT,FLAT,MSG('Accept dat' & |
            'a and close the window'),TIP('Accept data and close the window'),#ORIG(?OK),#SEQ(2), |
            #ORDINAL(17)
          BUTTON('&Cancel'),AT(300,147,49,14),USE(?Cancel),LEFT,ICON('WACANCEL.ICO'),FLAT, |
            MSG('Cancel operation'),TIP('Cancel operation'),#ORIG(?Cancel),#SEQ(3),#ORDINAL(18)
        END

