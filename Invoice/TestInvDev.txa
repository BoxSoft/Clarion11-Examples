QuickWindow WINDOW('Select a Product Record'),AT(,,358,198),FONT('Segoe UI',9,COLOR:Black, |
          FONT:regular,CHARSET:DEFAULT),RESIZE,CENTER,GRAY,IMM,MDI,HLP('SelectProduct'),SYSTEM
          LIST,AT(9,22,341,148),USE(?Browse:1),HVSCROLL,FORMAT('80L(2)|M~Product Code~L' & |
            '(2)@s100@80L(2)|M~Product Name~L(2)@s100@80L(2)|M~Description~L(2)@s255@64' & |
            'D(28)|M~Price~C(0)@n15.2@72R(2)|M~Quantity In Stock~C(0)@n-14@68R(2)|M~Reo' & |
            'rder Quantity~C(0)@n13@68D(32)|M~Cost~C(0)@n-15.2@80L(2)|M~Image~L(2)@s255@'), |
            FROM(Queue:Browse:1),IMM,MSG('Browsing the Product file'),#FIELDS(Pro:ProductCode, |
            Pro:ProductName,Pro:Description,Pro:Price,Pro:QuantityInStock,Pro:ReorderQuantity, |
            Pro:Cost,Pro:ImageFilename),#ORIG(?List),#SEQ(1),#ORDINAL(1)
          BUTTON('&Select'),AT(4,180,49,14),USE(?Select:2),LEFT,ICON('WASELECT.ICO'),FLAT, |
            MSG('Select the Record'),TIP('Select the Record'),#ORIG(?Select),#SEQ(2),#ORDINAL(2)
          SHEET,AT(4,4,350,172),USE(?CurrentTab),#ORIG(CurrentTab),#ORDINAL(3)
            TAB('&GuidKey'),USE(?Tab:2),#ORDINAL(4)
            END
            TAB('&ProductCodeKey'),USE(?Tab:3),#ORDINAL(5)
            END
            TAB('&ProductNameKey'),USE(?Tab:4),#ORDINAL(6)
            END
          END
          BUTTON('&Close'),AT(305,180,49,14),USE(?Close),LEFT,ICON('WACLOSE.ICO'),FLAT,MSG('Close Window'), |
            TIP('Close Window'),#ORIG(?Close),#SEQ(3),#ORDINAL(7)
        END

