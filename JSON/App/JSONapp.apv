SetJSONnames ROUTINE    !ROUTINE to set names to output in JSON object
    DATA
    CODE
      JSON.SetExternalName('STU:Number','StudentID')
      JSON.SetExternalName('STU:FirstName','First')
      JSON.SetExternalName('STU:LastName','Last')
      JSON.SetExternalName('STU:Address','Address')
      JSON.SetExternalName('STU:Address2','Address2')
      JSON.SetExternalName('STU:City','City')
      JSON.SetExternalName('STU:State','State')
      JSON.SetExternalName('STU:Zip','Zipcode')
      JSON.SetExternalName('STU:Telephone','Phone')
      JSON.SetExternalName('STU:Major','Major')
      JSON.SetExternalName('STU:GradYear','Graduation Year')
      EXIT
      

      
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
WriteJSONtoFile ROUTINE  !Routine to write a JSON string into ascii file
      Json.ClearObjectArray()
      Do SetJSONnames
      CLEAR(STU:Record,-1)
      SET(STU:KeyLastName)    !export in lastname order
      LOOP
          NEXT(STUDENTS)
          IF ERRORCODE()
              BREAK
          END
         Json.AddGroup(STU:Record)
      END
      ERR# = Json.ToJSONFile(PATH() & '\FromJSONText.txt')
      IF ERR#<>0
         MESSAGE('Error saving to the file.|Error:'&ERR#)
      ELSE      
         MESSAGE('JSON output was saved to ' & PATH() & '\FromJSONText.txt')
      END


