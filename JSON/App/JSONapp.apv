WHEN '?CopyToClipB'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
SETCLIPBOARD(CLIP(JSONstring))
[END]
[END]
WHEN '?ExportFileToJsonString'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
!Export FILE to JSON
CLEAR(JSONstring)       !CLEAR destination string
Json.ClearObjectArray()
DO SetJSONnames         !set the names to match our JSON schema
CLEAR(STU:Record,-1)
SET(STU:KeyLastName)    !export in lastname order
LOOP
    NEXT(STUDENTS)
    IF ERRORCODE()
        BREAK
    END
    Json.AddGroup(STU:Record)
END
JSONstring = JSON.ToJSON()
DISPLAY(?JSONstring)

    
[END]
[END]
WHEN '?WriteFileToJSONText'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
!Save JSON text to ascii FILE
DO WriteJSONtoFile
        
[END]
[END]
WHEN '?ReadJSONstringToQ'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
!convert JSON string into a QUEUE
!the .FromJSON method works with STRINGs
!so we put the CSTRING into a STRING
LocString &= NEW(STRING(LEN(CLIP(JSONstring))))
LocString = CLIP(JSONstring)
JSON.SetExternalName('GradYear', 'Graduation Year')
FREE(Queue:Browse)
DISPLAY(?List)
IF LEN(CLIP(JSONstring)) > 0
    JSON.FromJSON(LocString, Queue:Browse)    
END
DISPOSE(LocString)
DISPLAY(?List)    
[END]
[END]
WHEN '?ReadJSONtextFileToQ'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
!read JSON in ascii file into string and convert to a queue
IF NOT EXISTS('FromJSONText.txt')
   MESSAGE('The FromJSONText.txt file doesn''t exist')
END   
FREE(Queue:Browse)                                   !clear the queue
DISPLAY(?List)
Do SetJSONnames
JSON.SetExternalName('GradYear', 'Graduation Year')  !set mapping for JSON and field
ERR# = Json.FromJSONFile(PATH() & '\FromJSONText.txt', Queue:Browse)
IF ERR#<>0
   MESSAGE('Error reading the file.|Error:'&ERR#)
END
DISPLAY(?List)    
[END]
[END]
WHEN '?BUTTON1'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
!view json text file with default text program
IF EXISTS('FromJSONText.txt')
   RUN('FromJSONText.txt')
END   
[END]
[END]
WHEN '?ExportSubsetBtn'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
! Export only 3 fields from the File structure to JSON
! for ease of coding we hide all the fields then unhide the 3 we need
! alternatively we could have called JSON.HideGroupField(fieldname) for
! every field we did not want to be in the resulting JSON output
CLEAR(JSONstring)
JSON.HideAllGroupFields(STU:Record)
JSON.UnhideGroupField('STU:FirstName')
JSON.UnhideGroupField('STU:LastName')
JSON.UnhideGroupField('STU:Telephone')
DO SetJSONNames    !maps the names we want to use in our JSON text to the names in the FILE
DO WriteJSONtoFile !write the text file
JSON.UnhideAllGroupFields(STU:Record)   !clear the hidden state for other work


[END]
[END]
[END]
